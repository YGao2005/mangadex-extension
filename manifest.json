{
    "name": "Mangadex Extension",
    "description": "Keep track of your manga you're following.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "hello_extensions.png"
    },
    "host_permissions": [
      "https://api.mangadex.org/*"
    ]
}